{
  "buildOptions": {
    "incremental": true
  },
  "compilerOptions": {
    "composite": true,
    "incremental": true,
    "target": "ES2019",
    "module": "commonjs",
    "lib": [
      "ES2020"
    ],
    "sourceMap": false,
    "moduleResolution": "node",
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true
  }
}
